import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int a=sc.nextInt();
      int c=0;
      while(a>0){  //109
        int m=a%10; //9
        if(m==0) //9==0
        c++; //1
        a=a/10;
      }
      
      System.out.println(c);
    }
}
