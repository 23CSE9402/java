import java.io.*;
import java.util.*;

public class Travel {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        
        int x = sc.nextInt(); 
        int y = sc.nextInt(); 
        double solo = sc.nextDouble(); 
        double round = sc.nextDouble(); 
        double passcost = sc.nextDouble(); 

        
        if (solo > round || (2 * solo) < round) {
            System.out.println("Invalid Input");
            return;
        }

        
        double totalSoloRoundCost = (x * solo) + (y * round);

        
        if ( passcost< totalSoloRoundCost) {
            System.out.println("Monthly Pass");
        } else {
            System.out.println("Solo Trail + Roundabout Ride");
        }

        sc.close();
    }
}
