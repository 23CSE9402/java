public class swapwithoperators {

	public static void main(String[] args) {
		int a = 25, b = 90;

        System.out.println("Before swap: a = " + a + ", b = " + b);

        a = a + b;  
        b = a - b;  
        a = a - b;  

        System.out.println("After swap: a = " + a + ", b = " + b);

	}

}
