import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int b=sc.nextInt();
      String[] a={"zero","one","two","three","four","five","six","seven","eight","nine"};
      for(int i=0;i<10;i++){
        if(b==i){
          System.out.println(a[i]);
        }
      }
    }
}
