import java.util.Scanner;

public class palindrom {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		int num1=0;
		int digit=num;
		while(num>0) {
			int temp=num%10;
			num1=num1*10+temp;
			num=num/10;
		}
		System.out.println(num1);
		
		if(digit==num1){
			System.out.println("Palindrome");
		}
		else {
			System.out.println("not palindrome");
		}
		

	}
		
		

}
