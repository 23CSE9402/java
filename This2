class AAA{
		int age;
		String name;
		double cgpa;
		int marks;
		AAA(int age){
			this.age=age;
		}
		AAA(int age,String name){
			this(age);
			this.name=name;
		}
		AAA(int age,String name,double cgpa){
			this(age,name);
			this.cgpa=cgpa;
		}
		AAA(int age,String name,double cgpa,int marks){
			this(age,name,cgpa);
			this.marks=marks;
		}
		void display() {
			System.out.println(age);
			System.out.println(name);
			System.out.println(cgpa);
			System.out.println(marks);
		}
	}

	public class this2 {

		public static void main(String[] args) {
			new AAA(19).display();
			new AAA(19,"Kajol").display();
			new AAA(19,"Kajol",9).display();
			new AAA(19,"Kajol",9,99).display();
			

		}

	}
