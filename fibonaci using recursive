package day2;
import java.util.*;
public class fibonaci {
	int fibo(int n) {
		if(n==0) 
			return 0;
		else if(n==1)
			return 1;
	
		return fibo(n-1)+fibo(n-2);
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		fibonaci f=new fibonaci();
		int n=sc.nextInt();
		System.out.println(f.fibo(n));
	}

}
