import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int a=sc.nextInt();
      int b=sc.nextInt();
      int x=a;
      int y=b;
      int h=0,l=0;
      int r=Math.min(a,b);
      while(b!=0)
      {
        int t=b;
        b=a%b;
        a=t;
      }
      h=a;
      if(x==0 || y==0)
        l=0;
      else
        l=(x*y)/h;
        System.out.println(h+"\n"+l);
        
      }
      
    }
