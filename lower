public class lowertoupper {
	public static void main(String[] args) {

	        String input = "Hello";
	        String result = "";

	        for (int i = 0; i < input.length(); i++) {
	            char ch = input.charAt(i);
	            if (ch >= 'A' && ch <= 'Z') {
	                ch = (char)(ch + 32); 
	            }
	            result += ch;
	        }

	        System.out.println("Converted to lowercase: " + result);
	    }
	}
