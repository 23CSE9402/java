package day2;
import java.util.*;
public class digits {
	int sum(int n) {
			if(n==0) {
				return 0;
		}
		return (n%10)+sum(n/10);
			
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		digits m=new digits();
		int n=sc.nextInt();
		System.out.println(m.sum(n));
		
		
		

	}

}
